<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="ds_medicacion_receta" targetNamespace="http://tempuri.org/ds_medicacion_receta.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/ds_medicacion_receta.xsd"
	xmlns:mstns="http://tempuri.org/ds_medicacion_receta.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="ds_medicacion_receta" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="cima_medicacion">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id_medicacion" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />
							<xs:element name="pers_key" type="xs:int" minOccurs="0" />
							<xs:element name="drugselecode_key" type="xs:int" minOccurs="0" />
							<xs:element name="drugseledesc" type="xs:string" minOccurs="0" />
							<xs:element name="fecha" type="xs:dateTime" minOccurs="0" />
							<xs:element name="episcode" type="xs:string" minOccurs="0" />
							<xs:element name="posologia" type="xs:string" minOccurs="0" />
							<xs:element name="tipo_duracion" type="xs:string" minOccurs="0" />
							<xs:element name="duracion" type="xs:int" minOccurs="0" />
							<xs:element name="observaciones" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="ds_medicacion_recetaKey1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:cima_medicacion" />
			<xs:field xpath="mstns:id_medicacion" />
		</xs:unique>
	</xs:element>
</xs:schema>